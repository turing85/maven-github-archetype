<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd"
    name="github-archetype"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <requiredProperties>
    <requiredProperty key="projectUrl">
      <defaultValue>todo-define-later</defaultValue>
    </requiredProperty>
    <requiredProperty key="gitSshUrl">
      <defaultValue>todo-define-later</defaultValue>
    </requiredProperty>
    <requiredProperty key="gitSite">
      <defaultValue>todo-define-later</defaultValue>
    </requiredProperty>
    <requiredProperty key="sonarOrganizationKey">
      <defaultValue>todo-define-later</defaultValue>
    </requiredProperty>
    <requiredProperty key="sonarProjectKey">
      <defaultValue>todo-define-later</defaultValue>
    </requiredProperty>
  </requiredProperties>

  <fileSets>
    <fileSet encoding="UTF-8">
      <directory>.mvn/wrapper</directory>
      <includes>
        <include>*</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory/>
      <includes>
        <include>.gitignore</include>
        <include>google-style.xml</include>
        <include>LICENSE</include>
        <include>lombok.config</include>
        <include>mvnw</include>
        <include>mvnw.cmd</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory/>
      <includes>
        <include>.github/**/*</include>
        <include>bom/**/*</include>
        <include>core/**/*</include>
        <include>implementation-parent/**/*</include>
        <include>jacoco-aggregation/**/*</include>
      </includes>
    </fileSet>
  </fileSets>
</archetype-descriptor>